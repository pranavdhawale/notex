{
  "themeName": "NoteX Visual System",
  "version": "2.0.0",
  "description": "Dark mode only theme system for NoteX",
  "theme": {
    "displayName": "Graphite Liquid Glass",
    "colors": {
      "glass-bg": "rgba(20, 20, 22, 0.35)",
      "glass-border": "rgba(255, 255, 255, 0.10)",
      "glass-shadow": "0 8px 32px 0 rgba(0, 0, 0, 0.55)",
      "accent-color": "#D1D1D6",
      "text-main": "#F2F2F3",
      "text-dim": "rgba(242, 242, 243, 0.65)",
      "scrollbar-thumb": "rgba(255, 255, 255, 0.20)",
      "button-hover": "rgba(255, 255, 255, 0.12)",
      "bg-gradient": "#000000",
      "orb-1": "#2A2A2E",
      "orb-2": "#3A3A40",
      "orb-3": "#1C1C20",
      "glass-item-bg": "rgba(255, 255, 255, 0.03)",
      "btn-bg-idle": "rgba(255, 255, 255, 0.05)",
      "glass-border-dim": "rgba(255, 255, 255, 0.04)"
    }
  },
  "semanticColors": {
    "primary": "#007AFF",
    "success": "#34C759",
    "warning": "#FF9500",
    "error": "#FF3B30",
    "caution": "#FFCC00"
  },
  "grayScale": {
    "light": "#E5E5EA",
    "mid": "#8E8E93",
    "dark2": "#3A3A3C",
    "dark": "#1C1C1E"
  },
  "typography": {
    "fontFamily": "'Inter', system-ui, Avenir, Helvetica, Arial, sans-serif",
    "lineHeight": 1.55,
    "fontWeight": 400
  },
  "particles": {
    "color": "#FFFFFF",
    "count": 900,
    "spread": 10,
    "speed": 0.1,
    "baseSize": 100
  }
}
